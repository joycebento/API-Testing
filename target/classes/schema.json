{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "http://example.com/example.json",
	"type": "array",
	"default": [],
	"title": "Root Schema",
	"items": {
		"type": "object",
		"default": {},
		"title": "A Schema",
		"required": [
			"id",
			"user_id",
			"title",
			"due_on",
			"status"
		],
		"properties": {
			"id": {
				"type": "integer",
				"default": 0,
				"title": "The id Schema",
				"examples": [
					51635
				]
			},
			"user_id": {
				"type": "integer",
				"default": 0,
				"title": "The user_id Schema",
				"examples": [
					6930072
				]
			},
			"title": {
				"type": "string",
				"default": "",
				"title": "The title Schema",
				"examples": [
					"Brevis vis cernuus tantillus adficio repellat."
				]
			},
			"due_on": {
				"type": "string",
				"default": "",
				"title": "The due_on Schema",
				"examples": [
					"2024-06-01T00:00:00.000+05:30"
				]
			},
			"status": {
				"type": "string",
				"default": "",
				"title": "The status Schema",
				"examples": [
					"pending"
				]
			}
		},
		"examples": [
			{
				"id": 51635,
				"user_id": 6930072,
				"title": "Brevis vis cernuus tantillus adficio repellat.",
				"due_on": "2024-06-01T00:00:00.000+05:30",
				"status": "pending"
			}
		]
	},
	"examples": [
		[
			{
				"id": 51635,
				"user_id": 6930072,
				"title": "Brevis vis cernuus tantillus adficio repellat.",
				"due_on": "2024-06-01T00:00:00.000+05:30",
				"status": "pending"
			}
		]
	]
}